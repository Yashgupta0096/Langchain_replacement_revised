{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Poetry: Uvicorn",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["main:app", "--host", "0.0.0.0", "--port", "4000", "--reload"],
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/apps/server",
      // "python": "${workspaceFolder}/apps/server/.venv/bin/python"
    },
    {
      "name": "Python: Test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/test.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/apps/server"
    }
  ]
}
